{
  "name": "analos-nft-launcher",
  "version": "0.1.0",
  "description": "ðŸš€ Launch On Los (LOL) - No-code NFT launchpad for Analos blockchain. Create, mint, and trade NFTs with ease.",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "contracts",
    "backend",
    "frontend",
    "shared"
  ],
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "start": "turbo start",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "type-check": "turbo type-check"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "turbo": "^2.5.6",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dubie-eth/AnalosNFTLauncher.git"
  },
  "keywords": [
    "nft",
    "launchpad",
    "analos",
    "solana",
    "blockchain",
    "crypto",
    "defi",
    "web3",
    "metaplex",
    "anchor"
  ],
  "author": {
    "name": "Dubie-eth",
